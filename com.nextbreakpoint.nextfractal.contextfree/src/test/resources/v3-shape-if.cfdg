CFDG3

startshape Foo

path heptstar(number filled)
{
    MOVETO(cos (90 - 720 / 7), sin(90 - 720 / 7))
    loop 6 [r (720/7)]
        LINETO(0, 1)
    CLOSEPOLY()
    if (filled) FILL[a -0.5]
    STROKE[]
}

path box(number filled)
{
	//MOVETO(1,0)
    loop 4 [r (90)]
        LINETO(0, 1)
    CLOSEPOLY()
    if (filled) FILL[a -0.5]
    STROKE[]
}

shape Foo {
    box (1) [rotate 10]
